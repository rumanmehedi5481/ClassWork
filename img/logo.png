@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');

/* global Csss  */
body {
    font-family: "Poppins", sans-serif
}

.specail{
    display: none;
}
a {
    text-decoration: none;
}

ul li {
    list-style: none;
}

h2 {
    font-size: 45px;
    line-height: 60px;
    color: #1D1D1D;
    font-weight: 600;
}

p {
    font-size: 16px;
    line-height: 30px;
    color: #777777;
    font-weight: 400;
}

h3 {
    font-size: 25px;
    font-weight: 600;
    line-height: 37.5px;
    color: #4A4A4A;
}

h4 {
    font-size: 20px;
    line-height: 30px;
    color: #4A4A4A;
}

h5 {
    font-size: 18px;
    line-height: 27px;
    color: #289BDE;
}

/*************************** global Csss End************************  */
/* *************** Navbar Secion Start********************/
.nav-bar-part {
    padding: 25px 0;

}
.nav-bar-part .navbar-brand {
    font-size: 40px;
    color: #777777;
    font-weight: 600;
}

.nav-bar-part ul {
    margin: 0 auto;
}

.nav-bar-part ul li a {
    margin-right: 30px;
    font-size: 18px;
    color: #777777;
}

.nav-bar-part .nav-btn a {
    border: 1px solid #289BDE;
    padding: 13px 33px;
    border-radius: 5px;
    font-size: 20px;
    font-weight: 600;
    color: #289BDE;
    transition: 0.7s;
}

.nav-bar-part .nav-btn a:hover {
    color: #fff;
    background: #289BDE;
    transition: 0.7s;
}

/* *************** Navbar Section End********************/
/* *************** banner section Start********************/
.carousel-inner {
    width: 100%;
    height: 100%;

}

.banner .carousel-caption {
    text-align: center;
    position: absolute;
    top: 158px;
    left: 186px;
}

.banner .carousel-caption h1 {
    font-size: 65px;
    line-height: 85px;
    font-weight: 600;
    color: #fff
}

.banner .carousel-caption p {
    color: #fff;
    padding: 10px 120px;
    font-size: 18px;
}

.banner .banner-btn {
    padding-top: 30px;
}

.banner a {
    border: 1px solid #289BDE;
    padding: 13px 33px;
    border-radius: 5px;
    font-size: 20px;
    font-weight: 600;
    color: #fff;
    transition: 0.7s;
    background: #289BDE;

}

.banner a:hover {
    background: transparent;
    color: #289BDE;
    transition: 0.7s;
}

/* *************** banner section End********************/

/* *************** course section Start********************/
.course {
    padding: 130px 0;
}

.course .course-title {
    padding: 0px 250px;
    text-align: center;
}
.card{
    transition: 0.7s;
}
.card:hover{
    box-shadow: rgba(50, 50, 93, 0.25) 0px 13px 27px -5px, rgba(0, 0, 0, 0.3) 0px 8px 16px -8px;
    transition: 0.7s;
}
.card:hover img{
    box-shadow: rgba(50, 50, 93, 0.25) 0px 13px 27px -5px, rgba(0, 0, 0, 0.3) 0px 8px 16px -8px;
    transition: 0.7s;
}


.course .course-title p {
    padding-bottom: 50px;
}

.course .course-inner {
    margin-bottom: 25px;
}

.course .course-btn {
    padding-top: 50px;
}

.course .course-inner {
    cursor: pointer;
}

.course .course-btn a {
    border: 1px solid #289BDE;
    padding: 13px 33px;
    border-radius: 5px;
    font-size: 20px;
    font-weight: 600;
    color: #fff;
    transition: 0.7s;
    background: #289BDE;

}

.course .course-btn a:hover {
    background: transparent;
    color: #289BDE;
    transition: 0.7s;
}


/* *************** course section End********************/

/* ********************Learning Section Start************************* */
.learning {}

.learning .background-color {
    background: #F4FAFD;
    position: relative;
    padding: 130px 30px 79px;
}
.learning .learning-pic img{
    height: 806px;
    width: 600px;
}
.learning .learning-content{
   
}
.learning .inner-contetn {
    display: flex;
    justify-content: space-between;
    padding: 30px 0;

}

.learning .learning-btn a {
    border: 1px solid #289BDE;
    padding: 13px 33px;
    border-radius: 5px;
    font-size: 20px;
    font-weight: 600;
    color: #fff;
    transition: 0.7s;
    background: #289BDE;

}
.learning-content .hide {
    display: none;
}
.learning-content .tab-p{
    display: none;
}
.learning-btn {
    padding-top: 10px;
}

.learning .learning-btn a:hover {
    background: transparent;
    color: #289BDE;
    transition: 0.7s;
}


/* ********************Learning Section End************************* */

/* ***********************Redy section start ******************** */
.ready {
    padding-top: 130px;
}

.ready-row {
    height: 300px;
    line-height: 300px;
    background: #289BDE;

}

.ready .ready-row .ready-title {
    padding: 80px 100px;
}

.ready .ready-row .ready-title h2 {
    color: #fff;
    margin-bottom: 20px;
}

.ready .ready-row .ready-title p {
    color: #fff;
}

.ready .ready-row .ready-btn a {
    border: 1px solid #289BDE;
    padding: 13px 33px;
    border-radius: 5px;
    font-size: 20px;
    font-weight: 600;
    color: #289BDE;
    transition: 0.7s;
    background: #fff;

}

.ready .ready-btn a:hover {
    background: #289BDE;
    color: #fff;
    transition: 0.7s;
    border: 1px solid #fff;
}

/* ***********************Redy section end ******************** */
/* ******************Students section start********************** */
.students {
    padding: 130px 0;
}

.students .students-title {
    padding-bottom: 50px;
}

.students .students-title p {
    width: 45%;
}

.students .students-title h2 {
    width: 50%;
}

.students .studnets-profile {
    box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 1px 3px 1px;
    border-radius: 5px;
    transition: 0.7s;
    cursor: pointer;
}
.students .studnets-profile:hover{
    box-shadow: rgba(50, 50, 93, 0.25) 0px 13px 27px -5px, rgba(0, 0, 0, 0.3) 0px 8px 16px -8px;
    transition: 0.7s;
}

.students .studnets-profile img {
    width: 100%;
}

.students .studnets-profile .students-details {
    padding: 20px 20px;

}

.students .student-btn {
    padding-top: 50px;
    text-align: center;
}

.students .student-btn a {
    border: 1px solid #289BDE;
    padding: 13px 33px;
    border-radius: 5px;
    font-size: 20px;
    font-weight: 600;
    color: #fff;
    transition: 0.7s;
    background: #289BDE;

}

.students .student-btn a:hover {
    background: #fff;
    color: #289BDE;
    transition: 0.7s;
    border: 1px solid #289BDE;
}

/* ******************Students section End********************** */
/* *********************Feed back section start********************* */
.feed-back {
    padding-bottom: 130px;
}

.feed-back .feed-title {
    text-align: center;
}

.feed-back .feed-title p {
    padding: 0px 350px;
}

.feed-back .icon {
    display: flex;
    justify-content: space-between;
}

.feed-back .icon .quote i {
    font-size: 56px;
    color: #D2D2D2;
}

.feed-back .icon .star i {
    color: #f59e2e;
}

.feed-back .item {
    padding: 40px 35px;
    border: 1px solid #E8E8E8;
    border-radius: 5px;
    margin: 0px 12px;
    margin-top: 50px;
    box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 2px 6px 2px;
}


.feed-back .item .profile .pic img {
    width: 60px;
    margin-right: 20px;
}

.feed-back .item .profile {
    display: flex;
}

.feed-back .item .text p {
    padding-top: 20px;
}

.owl-theme .owl-dots .owl-dot {
    display: none;
}
.feedback-btns{
    display: none;
}

/* **********************Feed Back Section End******************** */

/* *************************Acrodin Section Start*************************** */
.acrodin .acrodin-title {
    text-align: center;
}

.acrodin .acrodin-title p {
    padding: 0px 350px;
    padding-top: 20px;
}

.acrodin .acrodin-inner {
    padding: 50px 225px;
}

.acrodin .acrodin-inner p {
    padding: 0px 15px;
}

.acrodin .acrodin-inner .accordion-item {
    margin-bottom: 20px;
    box-shadow: rgba(50, 50, 93, 0.25) 0px 2px 5px -1px, rgba(0, 0, 0, 0.3) 0px 1px 3px -1px;
}

/* *************************Acrodin Section End*************************** */


/* *********************************Sponsor section Start********************* */
.sponsor {
    padding-top: 60px;
    padding-bottom: 130px;
}

.sponsor .sponsor-title {
    text-align: center;
}

.sponsor .sponsor-title h2 {
    padding-bottom: 35px;
}

/* *********************************Sponsor section End********************* */
/* *********************Fotter section start************************ */
.fotter {
    padding: 130px 0px;
    background: #161616;
    color: #fff;
    text-align: center;
}

.fotter p {
    padding: 0px 462px;
    color: #fff;
}

.fotter h2 {
    color: #fff;
    font-size: 40px;
}

.fotter .fotter-icon {
    display: flex;
    justify-content: center;
    padding-top: 20px;
}

.fotter .fotter-icon a {
    margin-right: 20px;
    color: #fff;
    font-size: 16px;
    border: 1px solid #242424;
    padding: 8px 12px;
    border-radius: 50%;
    background: #242424;
    transition: 0.7s;
}

.fotter .fotter-icon a:hover {
    background: #fff;
    color: #289BDE;
    transition: 0.7s;

}


/* *********************Fotter section End ************************ */

